.aside

	.title
		margin-bottom 15px

	&__item
		margin-bottom 30px

	&__map
		height 325px
		width 100%
		border 2px solid #b2b2b2
		margin-bottom 20px

	&__btn
		padding 0 8px
		min-width 136px
		text-decoration none
		line-height 25px
		background-color #f90706
		color #fff
		display inline-block
		text-align center
		border 1px solid
		box-shadow 0 0 10px 2px rgba(#000, .15)
